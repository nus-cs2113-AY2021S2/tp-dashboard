          } else {"},{"lineNumber":148,"author":{"gitId":"SimBowen"},"content":"                System.out.print(numberToBlock.get(path.get(i)));"},{"lineNumber":149,"author":{"gitId":"SimBowen"},"content":"            }"},{"lineNumber":150,"author":{"gitId":"SimBowen"},"content":"        }"},{"lineNumber":151,"author":{"gitId":"SimBowen"},"content":"    }"},{"lineNumber":152,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":153,"author":{"gitId":"SimBowen"},"content":"    // a modified version of BFS that stores predecessor"},{"lineNumber":154,"author":{"gitId":"SimBowen"},"content":"    // of each vertex in array pred"},{"lineNumber":155,"author":{"gitId":"SimBowen"},"content":"    // and its distance from source in array dist"},{"lineNumber":156,"author":{"gitId":"SimBowen"},"content":"    private static boolean BFS(ArrayList\u003cArrayList\u003cInteger\u003e\u003e adj, int src,"},{"lineNumber":157,"author":{"gitId":"SimBowen"},"content":"                               int dest, int v, int[] pred, int[] dist)"},{"lineNumber":158,"author":{"gitId":"SimBowen"},"content":"    {"},{"lineNumber":159,"author":{"gitId":"SimBowen"},"content":"        // a queue to maintain queue of vertices whose"},{"lineNumber":160,"author":{"gitId":"SimBowen"},"content":"        // adjacency list is to be scanned as per normal"},{"lineNumber":161,"author":{"gitId":"SimBowen"},"content":"        // BFS algorithm using LinkedList of Integer type"},{"lineNumber":162,"author":{"gitId":"SimBowen"},"content":"        LinkedList\u003cInteger\u003e queue \u003d new LinkedList\u003cInteger\u003e();"},{"lineNumber":163,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":164,"author":{"gitId":"SimBowen"},"content":"        // boolean array visited[] which stores the"},{"lineNumber":165,"author":{"gitId":"SimBowen"},"content":"        // information whether ith vertex is reached"},{"lineNumber":166,"author":{"gitId":"SimBowen"},"content":"        // at least once in the Breadth first search"},{"lineNumber":167,"author":{"gitId":"SimBowen"},"content":"        boolean[] visited \u003d new boolean[v];"},{"lineNumber":168,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":169,"author":{"gitId":"SimBowen"},"content":"        // initially all vertices are unvisited"},{"lineNumber":170,"author":{"gitId":"SimBowen"},"content":"        // so v[i] for all i is false"},{"lineNumber":171,"author":{"gitId":"SimBowen"},"content":"        // and as no path is yet constructed"},{"lineNumber":172,"author":{"gitId":"SimBowen"},"content":"        // dist[i] for all i set to infinity"},{"lineNumber":173,"author":{"gitId":"SimBowen"},"content":"        for (int i \u003d 0; i \u003c v; i++) {"},{"lineNumber":174,"author":{"gitId":"SimBowen"},"content":"            visited[i] \u003d false;"},{"lineNumber":175,"author":{"gitId":"SimBowen"},"content":"            dist[i] \u003d Integer.MAX_VALUE;"},{"lineNumber":176,"author":{"gitId":"SimBowen"},"content":"            pred[i] \u003d -1;"},{"lineNumber":177,"author":{"gitId":"SimBowen"},"content":"        }"},{"lineNumber":178,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":179,"author":{"gitId":"SimBowen"},"content":"        // now source is first to be visited and"},{"lineNumber":180,"author":{"gitId":"SimBowen"},"content":"        // distance from source to itself should be 0"},{"lineNumber":181,"author":{"gitId":"SimBowen"},"content":"        visited[src] \u003d true;"},{"lineNumber":182,"author":{"gitId":"SimBowen"},"content":"        dist[src] \u003d 0;"},{"lineNumber":183,"author":{"gitId":"SimBowen"},"content":"        queue.add(src);"},{"lineNumber":184,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":185,"author":{"gitId":"SimBowen"},"content":"        // bfs Algorithm"},{"lineNumber":186,"author":{"gitId":"SimBowen"},"content":"        while (!queue.isEmpty()) {"},{"lineNumber":187,"author":{"gitId":"SimBowen"},"content":"            int u \u003d queue.remove();"},{"lineNumber":188,"author":{"gitId":"SimBowen"},"content":"            for (int i \u003d 0; i \u003c adj.get(u).size(); i++) {"},{"lineNumber":189,"author":{"gitId":"SimBowen"},"content":"                if (!visited[adj.get(u).get(i)]) {"},{"lineNumber":190,"author":{"gitId":"SimBowen"},"content":"                    visited[adj.get(u).get(i)] \u003d true;"},{"lineNumber":191,"author":{"gitId":"SimBowen"},"content":"                    dist[adj.get(u).get(i)] \u003d dist[u] + 1;"},{"lineNumber":192,"author":{"gitId":"SimBowen"},"content":"                    pred[adj.get(u).get(i)] \u003d u;"},{"lineNumber":193,"author":{"gitId":"SimBowen"},"content":"                    queue.add(adj.get(u).get(i));"},{"lineNumber":194,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":195,"author":{"gitId":"SimBowen"},"content":"                    // stopping condition (when we find"},{"lineNumber":196,"author":{"gitId":"SimBowen"},"content":"                    // our destination)"},{"lineNumber":197,"author":{"gitId":"SimBowen"},"content":"                    if (adj.get(u).get(i) \u003d\u003d dest)"},{"lineNumber":198,"author":{"gitId":"SimBowen"},"content":"                        return true;"},{"lineNumber":199,"author":{"gitId":"SimBowen"},"content":"                }"},{"lineNumber":200,"author":{"gitId":"SimBowen"},"content":"            }"},{"lineNumber":201,"author":{"gitId":"SimBowen"},"content":"        }"},{"lineNumber":202,"author":{"gitId":"SimBowen"},"content":"        return false;"},{"lineNumber":203,"author":{"gitId":"SimBowen"},"content":"    }"},{"lineNumber":204,"author":{"gitId":"SimBowen"},"content":""},{"lineNumber":205,"author":{"gitId":"SimBowen"},"content":"}"}],"authorContributionMap":{"SimBowen":205}}]
